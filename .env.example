# Ontoiq System - Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to Git!

# ===========================================
# PostgreSQL
# ===========================================
POSTGRES_DB=ontoiq
POSTGRES_USER=ontoiq
POSTGRES_PASSWORD=your_secure_password_here

# ===========================================
# OpenClaw Gateway
# ===========================================
OPENCLAW_GATEWAY_TOKEN=your_openclaw_gateway_token_here

# ===========================================
# Telegram Bot
# ===========================================
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
USER_TELEGRAM_ID=your_telegram_user_id_here

# ===========================================
# n8n
# ===========================================
N8N_USER=admin
N8N_PASSWORD=your_n8n_password_here

# ===========================================
# Syncthing (legacy - may remove)
# ===========================================
SYNCTHING_GUI_USER=admin
SYNCTHING_GUI_PASSWORD=your_syncthing_password_here

# ===========================================
# Production Specific
# ===========================================
N8N_HOST=n8n.yourdomain.com
N8N_PROTOCOL=https
WEBHOOK_URL=https://n8n.yourdomain.com/

# ===========================================
# Cloudflare Tunnel Tokens
# ===========================================
CLOUDFLARED_DEV_TOKEN=your_dev_tunnel_token_here
CLOUDFLARED_TUNNEL_TOKEN=your_prod_tunnel_token_here

# ===========================================
# Kimi AI Configuration
# ===========================================
KIMI_API_KEY=your_kimi_api_key_here
KIMI_MODEL=kimi-coding/k2p5
